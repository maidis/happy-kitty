    <SpecFile>
        <Source>
            <Name>airstrike</Name>
            <Homepage>http://icculus.org/airstrike</Homepage>
            <Packager>
                <Name>Anıl Özbek</Name>
                <Email>ozbekanil@gmail.com</Email>
            </Packager>
            <License>GPLv2</License>
            <IsA>app:gui</IsA>
            <PartOf>game.arcade</PartOf>
            <Summary xml:lang="en">A 2d dogfight game</Summary>
            <Summary xml:lang="tr">İki boyutlu itdalaşı oyunu</Summary>
            <Description xml:lang="en">Airstrike is a 2d dogfight game in the tradition of the Intellivision and Amiga games Biplanes and BIP. It features a robust physics engine and several other extensions of the original games.</Description>
            <Description xml:lang="tr">Airstrike, Intellivision ve Amiga oyunları Biplanes ve BIP&apos;e benzeyen bir 2b itdalaşı oyunudur. Güçlü bir fizik motoruna ve ek pek çok özelliğe sahiptir.</Description>
            <Icon>airstrike</Icon>
            <Archive type="targz" sha1sum="799f02b6f504ca0564e9fd18e9da8062c10dabef">http://icculus.org/airstrike/airstrike-pre6a-src.tar.gz</Archive>
            <BuildDependencies>
                <Dependency>libsdl-devel</Dependency>
                <Dependency>sdl-image-devel</Dependency>
                <Dependency>sdl-mixer-devel</Dependency>
            </BuildDependencies>
            <Patches>
                <Patch level="1">airstrikerc.diff</Patch>
                <Patch level="1">fix-buffer-overflow.diff</Patch>
                <Patch level="1">fix_paths.diff</Patch>
                <Patch level="1">fix-red-baron-has-unlimited-bombs.diff</Patch>
                <Patch level="1">fix-segfault-on-close.diff</Patch>
                <Patch level="1">fix-spawn-level.diff</Patch>
                <Patch level="1">fix-unusable-on-amd64.diff</Patch>
                <Patch level="1">manpage.diff</Patch>
                <Patch>system-flags.diff</Patch>
            </Patches>
            <SourceURI>game/arcade/airstrike/pspec.xml</SourceURI>
        </Source>
        <Package>
            <Name>airstrike</Name>
            <RuntimeDependencies>
                <Dependency>libsdl</Dependency>
                <Dependency>sdl-image</Dependency>
                <Dependency>sdl-mixer</Dependency>
            </RuntimeDependencies>
            <Files>
                <Path fileType="executable">/usr/bin</Path>
                <Path fileType="config">/etc</Path>
                <Path fileType="data">/usr/share/applications</Path>
                <Path fileType="data">/usr/share/pixmaps</Path>
                <Path fileType="data">/usr/share/airstrike</Path>
                <Path fileType="man">/usr/share/man</Path>
                <Path fileType="doc">/usr/share/doc</Path>
            </Files>
            <AdditionalFiles>
                <AdditionalFile target="/usr/share/applications/airstrike.desktop" permission="0644" owner="root">airstrike.desktop</AdditionalFile>
                <AdditionalFile target="/usr/share/pixmaps/airstrike.png" permission="0644" owner="root">airstrike.png</AdditionalFile>
            </AdditionalFiles>
        </Package>
        <History>
            <Update release="1">
                <Date>2011-01-23</Date>
                <Version>0.99</Version>
                <Comment>First release</Comment>
                <Name>Anıl Özbek</Name>
                <Email>ozbekanil@gmail.com</Email>
            </Update>
        </History>
    </SpecFile>
    <SpecFile>
        <Source>
            <Name>box2d</Name>
            <Homepage>http://www.box2d.org</Homepage>
            <Packager>
                <Name>Anıl Özbek</Name>
                <Email>ozbekanil@gmail.com</Email>
            </Packager>
            <License>ZLIB</License>
            <IsA>library</IsA>
            <PartOf>library</PartOf>
            <Summary xml:lang="en">2D rigid body simulation library</Summary>
            <Summary xml:lang="tr">2B katı cisin simülasyon kütüphanesi</Summary>
            <Description xml:lang="en">Box2D is a 2D rigid body simulation library for games.</Description>
            <Description xml:lang="tr">Box2D, oyunlar için bir 2B katı cisin simülasyon kütüphanesidir.</Description>
            <Archive type="zip" sha1sum="b1f09f38fc130ae6c17e1767747a3a82bf8e517f">http://box2d.googlecode.com/files/Box2D_v2.1.2.zip</Archive>
            <BuildDependencies>
                <Dependency>freeglut-devel</Dependency>
            </BuildDependencies>
            <SourceURI>library/box2d/pspec.xml</SourceURI>
        </Source>
        <Package>
            <Name>box2d</Name>
            <RuntimeDependencies>
                <Dependency>freeglut</Dependency>
            </RuntimeDependencies>
            <Files>
                <Path fileType="library">/usr/lib</Path>
                <Path fileType="header">/usr/include</Path>
                <Path fileType="data">/usr/lib/Box2D</Path>
                <Path fileType="doc">/usr/share/doc</Path>
            </Files>
        </Package>
        <History>
            <Update release="1">
                <Date>2010-09-12</Date>
                <Version>2.1.2</Version>
                <Comment>First release</Comment>
                <Name>Anıl Özbek</Name>
                <Email>ozbekanil@gmail.com</Email>
            </Update>
        </History>
    </SpecFile>
